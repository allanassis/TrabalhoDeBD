<div id="div_cadastrar">
  <button id="btn_show_form_cli">cliente</button>
  <button id="btn_show_form_fun">funcionario</button>
  <button id="btn_show_form_pro">produtos</button>
        <form id="form_cli" style="border:2px solid black">
          <h2>cliente</h2>
          <div class="form-group">
              <label for="email">Nome:</label>
              <input type="text" class="form-control" id="cli_nome">
          </div>
          <div class="form-group">
              <label for="email">CPF:</label>
              <input type="text" class="form-control" id="cli_cpf">
          </div>
          <div class="form-group">
              <label for="email">Endere√ßo:</label>
              <input type="text" class="form-control" id="cli_endereco">
          </div>
          <div class="form-group">
              <label for="email">Data de Nascimento:</label>
              <input type="date" class="form-control" id="cli_dataNascimento">
          </div>
          <div class="form-group">
              <label for="email">Telefone:</label>
              <input type="text" class="form-control" id="cli_telefone1">
          </div>
          <div class="form-group">
              <label for="email">Telefone(2):</label>
              <input type="text" class="form-control" id="cli_telefone2">
          </div>
          
          <button class="btn btn-default" id="btn_cli_cadastrar">Cadastrar</button>
          <button class="btn btn-default" id="btn_cli_editar">Editar</button>
        </form>
    
        <form id="form_fun" style="border:2px solid black">
            <h2>funcionario</h2>
            <input type="text" id="fun_nome">
            <input type="text" id="fun_cpf">
            <input type="text" id="fun_tipo">
            <input type="date" id="fun_dataAdmissao">
            <input type="text" id="fun_senhaAcesso">
            
            <button id="btn_fun_cadastrar">cadastrar</button>
            <button id="btn_fun_editar">editar</button>
        </form>
    
          <form id="form_pro" style="border:2px solid black">
              <h2>produto</h2>
              <input type="text" id="pro_descricao">
              <input type="text" id="pro_valor">
              <input type="number" id="pro_saldoEstoque">
              
              <button id="btn_pro_cadastrar">cadastrar</button>
              <button id="btn_pro_editar">editar</button>
            </form>
          </div>
        